[Desktop Entry]
_Name=@APP_NAME@
_Comment=Organize your photos
Exec=@EXEC_NAME@ %U
Icon=multimedia-photo-manager
_GenericName=Photo Manager
_Keywords=album;cameras;crop;edit;enhance;export;gallery;images;import;organize;photographs;photos;pictures;photography;print;publish;rotate;share;tags;video
Terminal=false
Type=Application
StartupNotify=true
MimeType=x-content/image-dcf;
Categories=Graphics;Photography;GNOME;GTK;
Actions=AboutDialog;
X-GNOME-UsesNotifications=false

[Desktop Action AboutDialog]
_Name=About @APP_NAME@
Exec=@EXEC_NAME@ --about
